package com.javalec.list;

import java.util.ArrayList;

public class MainClass {
	public static void main(String[] args) {
		ArrayList<String> arrList = new ArrayList<String>();
		
		arrList.add("str1");
		arrList.add("str2");
		arrList.add("str3");
		arrList.add("str4");
		
		System.out.println(arrList.toString());
		
	}
}
